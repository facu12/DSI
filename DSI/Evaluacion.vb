Public Class Evaluacion

    Private comentario As String
    Private puntajeAsignado As Double

    Public Function getComentario() As String
        Return comentario
    End Function

    Public Sub setComentario(comentario As String)
        comentario = comentario
    End Sub

    Public Function getPuntajeAsignado() As Double
        Return puntajeAsignado
    End Function

    Public Sub setPuntajeAsignado(puntajeAsignado As Double)
        puntajeAsignado = puntajeAsignado
    End Sub
End Class
